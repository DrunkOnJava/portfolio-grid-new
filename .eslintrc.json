{
    "extends": [
        "next/core-web-vitals",
        "next/typescript"
    ],
    "rules": {
        "react/display-name": "off",
        "@typescript-eslint/no-unused-vars": [
            "warn",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ],
        "no-console": [
            "warn",
            {
                "allow": ["warn", "error"]
            }
        ]
    }
}